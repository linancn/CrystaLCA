{
  "name": "tiangonglca",
  "version": "0.0.7",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "openapi": "umi openapi",
    "postinstall": "umi g tmp",
    "precommit": "lint-staged",
    "pretest": "node ./tests/beforeTest",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=test umi dev",
    "start": "cross-env UMI_UI=none umi dev",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "test": "umi test",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.3.2",
    "@ant-design/icons": "^4.5.0",
    "@ant-design/pro-card": "^1.15.5",
    "@ant-design/pro-cli": "^2.0.2",
    "@ant-design/pro-descriptions": "^1.6.8",
    "@ant-design/pro-form": "^1.42.1",
    "@ant-design/pro-layout": "^6.15.3",
    "@ant-design/pro-list": "^1.10.9",
    "@ant-design/pro-table": "^2.30.8",
    "@antv/auto-chart": "^2.0.2",
    "@antv/layout": "^0.1.19",
    "@antv/s2": "^1.2.0",
    "@antv/xflow": "^1.0.21",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/react-color": "^3.0.5",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react": "^17.0.0",
    "ahooks": "^3.0.1",
    "antd": "^4.14.0",
    "carlo": "^0.9.46",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "is-electron": "^2.2.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "localforage": "^1.9.0",
    "lodash": "^4.17.11",
    "mockjs": "^1.1.0",
    "moment": "^2.25.3",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "omit.js": "^2.0.2",
    "prettier": "^2.6.2",
    "puppeteer-core": "^8.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.0",
    "react-flow-renderer": "^9.6.3",
    "react-helmet-async": "^1.0.4",
    "react": "^17.0.0",
    "reactcss": "^1.2.3",
    "stylelint": "^13.0.0",
    "typescript": "^4.2.2",
    "umi": "^v3.4.0"
  },
  "devDependencies": {
    "@umijs/openapi": "^1.1.14",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/plugin-openapi": "^1.2.0",
    "@umijs/preset-react": "^1.7.4",
    "@umijs/yorkie":"^2.0.5",
    "react-dev-inspector": "^1.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "gitHooks": {
    "pre-commit": "yarn lint",
    "commit-msg": "fabric verify-commit"
  }
}
